{
  "name": "@chicago-forest/libp2p-transport",
  "version": "0.1.0",
  "description": "Production P2P networking layer using libp2p for Chicago Forest Network",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.js",
      "types": "./dist/node.d.ts"
    },
    "./transport": {
      "import": "./dist/transport-adapter.mjs",
      "require": "./dist/transport-adapter.js",
      "types": "./dist/transport-adapter.d.ts"
    },
    "./discovery": {
      "import": "./dist/discovery.mjs",
      "require": "./dist/discovery.js",
      "types": "./dist/discovery.d.ts"
    },
    "./pubsub": {
      "import": "./dist/pubsub.mjs",
      "require": "./dist/pubsub.js",
      "types": "./dist/pubsub.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chicago-forest/p2p-core": "workspace:*",
    "@chicago-forest/shared-types": "workspace:*",
    "@chainsafe/libp2p-gossipsub": "^14.1.0",
    "@chainsafe/libp2p-noise": "^16.0.0",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@libp2p/bootstrap": "^11.0.7",
    "@libp2p/circuit-relay-v2": "^2.1.5",
    "@libp2p/dcutr": "^2.0.7",
    "@libp2p/identify": "^3.0.7",
    "@libp2p/interface": "^2.2.1",
    "@libp2p/kad-dht": "^13.0.7",
    "@libp2p/mdns": "^11.0.7",
    "@libp2p/ping": "^2.0.7",
    "@libp2p/tcp": "^10.0.7",
    "@libp2p/webrtc": "^5.0.13",
    "@libp2p/websockets": "^9.0.7",
    "@multiformats/multiaddr": "^12.3.1",
    "libp2p": "^2.2.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chicago-forest",
    "libp2p",
    "p2p",
    "networking",
    "decentralized"
  ]
}
