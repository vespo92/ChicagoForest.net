{
  "name": "@chicago-forest/doc-generator",
  "version": "0.1.0",
  "description": "Automated API documentation, README generation, and architecture diagrams for Chicago Forest Network",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./extractors": {
      "import": "./dist/extractors/index.mjs",
      "require": "./dist/extractors/index.js",
      "types": "./dist/extractors/index.d.ts"
    },
    "./generators": {
      "import": "./dist/generators/index.mjs",
      "require": "./dist/generators/index.js",
      "types": "./dist/generators/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "generate": "tsx src/cli.ts"
  },
  "dependencies": {
    "@chicago-forest/shared-types": "workspace:*",
    "@chicago-forest/canopy-api": "workspace:*",
    "eventemitter3": "^5.0.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2",
    "vitest": "^1.5.0",
    "tsx": "^4.7.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chicago-forest",
    "documentation",
    "api-docs",
    "readme-generator",
    "mermaid-diagrams",
    "changelog-automation"
  ]
}
