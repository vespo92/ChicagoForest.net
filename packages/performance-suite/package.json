{
  "name": "@chicago-forest/performance-suite",
  "version": "0.1.0",
  "description": "Performance benchmarking, scalability testing, and regression detection for Chicago Forest Network",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./benchmarks": {
      "import": "./dist/benchmarks.mjs",
      "require": "./dist/benchmarks.js",
      "types": "./dist/benchmarks.d.ts"
    },
    "./profilers": {
      "import": "./dist/profilers.mjs",
      "require": "./dist/profilers.js",
      "types": "./dist/profilers.d.ts"
    },
    "./reporters": {
      "import": "./dist/reporters.mjs",
      "require": "./dist/reporters.js",
      "types": "./dist/reporters.d.ts"
    },
    "./load": {
      "import": "./dist/load.mjs",
      "require": "./dist/load.js",
      "types": "./dist/load.d.ts"
    },
    "./latency": {
      "import": "./dist/latency.mjs",
      "require": "./dist/latency.js",
      "types": "./dist/latency.d.ts"
    },
    "./baseline": {
      "import": "./dist/baseline.mjs",
      "require": "./dist/baseline.js",
      "types": "./dist/baseline.d.ts"
    },
    "./gates": {
      "import": "./dist/gates.mjs",
      "require": "./dist/gates.js",
      "types": "./dist/gates.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "bench": "tsx src/cli.ts"
  },
  "dependencies": {
    "@chicago-forest/shared-types": "workspace:*",
    "@chicago-forest/mycelium-core": "workspace:*",
    "@chicago-forest/p2p-core": "workspace:*",
    "@chicago-forest/canopy-api": "workspace:*",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "tsx": "^4.7.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "chicago-forest",
    "performance",
    "benchmarking",
    "scalability",
    "profiling",
    "regression-detection",
    "load-testing",
    "latency",
    "ci-cd"
  ],
  "author": "Chicago Forest Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vespo92/ChicagoForest.net"
  }
}
