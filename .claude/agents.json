  "mergeOrder": [
    { "phase": 1, "agents": [9], "reason": "Test utilities required by all other agents" },
    { "phase": 2, "agents": [5, 6, 17], "reason": "Independent packages with no deps (Archivist, Beacon, Ambassador)" },
    { "phase": 3, "agents": [1, 11], "reason": "Core network foundation + Source verification (Mycelia, Verifier)" },
    { "phase": 4, "agents": [2, 12], "reason": "Network propagation + Content generation (Rhizome, Scribe)" },
    { "phase": 5, "agents": [4, 16], "reason": "Security layer + Security auditing (Sentinel, Auditor)" },
    { "phase": 6, "agents": [8, 13], "reason": "Governance + Compliance enforcement (Delegate, Compliance)" },
    { "phase": 7, "agents": [3, 18, 20], "reason": "Cross-repo bridges + Data sync + Ecosystem validation (Symbiont, Synchronizer, Validator)" },
    { "phase": 8, "agents": [7, 14, 19], "reason": "API layer + Documentation + Performance (Nexus, Prophet, Benchmarker)" },
    { "phase": 9, "agents": [10, 15], "reason": "Frontend + Network simulation (Oracle, Simulator)" }
  ],
  "branchNaming": {
    "pattern": "claude/agent-{id}-{codename}-{session_id}",
    "examples": [
      "claude/agent-1-mycelia-abc123",
      "claude/agent-5-archivist-def456"
    ]
  },
  "commitConvention": {
    "pattern": "feat(agent-{id}): {description}",
    "types": ["feat", "fix", "docs", "test", "refactor"]
  },
  "qualityGates": {
    "typescript": "strict",
    "testCoverage": 80,
    "linting": "eslint",
    "formatting": "prettier"
  }
}
